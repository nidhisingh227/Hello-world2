#include <stdio.h>

int main(void) {
	// your code goes here
	int a,b,c;
	
		scanf("%d%d",&a,&b);
		if(a>=1&&a<=10000 && b>=1&&b<=10000)
		{
			a=a-b;
			c=a%10;
			if(c>=0 && c<=8)
			{
				a=a+1;
			}
			else 
			if(c==9)
			{
				a=a-1;
			}
		}
			printf("%d\n",a);
	
	return 0;
}
